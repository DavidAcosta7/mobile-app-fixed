{
  "expo": {
    "name": "FluxPay",
    "slug": "fluxpay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2563EB"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.fluxpay.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./logo.png",
        "backgroundColor": "#2563EB"
      },
      "permissions": [
        "POST_NOTIFICATIONS",
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./logo.png",
          "color": "#2563EB"
        }
      ]
    ],
    "scheme": "fluxpay",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "33c963c1-5760-4fa9-bdf6-6554dbdc3118"
      }
    }
  }
}
